//Write a C++ program to calculate the sum of two numbers.
#include<iostream>
using namespace std;
int main()
{
    int a,b,c;
    cout<<"Enter the First Number-"<<endl;
    cin>>a;
    cout<<"Enter the Second Number-"<<endl;
    cin>>b;
    c=a+b;
    cout<<"The sum of two Number:-"<<c;
    return 0;


}
