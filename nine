//Write a C++ program to find the maximum of two numbers
#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cout<<"Enter the number of a:-";
    cin>>a;
    cout<<"Enter the number of b:-";
    cin>>b;
    if(a<b)
    {
        cout<<"b is greater the a";
    }
    else
    {
cout<<"a is greater the b";
    }
    return 0;

}
